{
  "$schema": "https://json-schema.org/draft-07/schema",
  "description": "Car object schema",
  "type": "object",
  "required": [
    "regNumber",
    "login",
    "motorTypeId",
    "markModelId"
  ],
  "properties": {
    "regNumber": {
      "description": "Регистрационный номер",
      "type": "string",
      "maxLength": 10
    },
    "login": {
      "description": "Логин клиента",
      "type": "string",
      "maxLength": 100
    },
    "motorTypeId": {
      "description": "Ид типа мотора.",
      "type": "string",
      "format": "uuid",
      "example": "ba54b3b7-1dab-41a8-976a-6bfe68d67d41"
    },
    "markModelId": {
      "description": "Ид марки модели.",
      "type": "string",
      "format": "uuid",
      "example": "ef8ac859-3ab8-49de-9d39-de0effc16a3a"
    }
  }
}